  <step>
    <name>Select Pedidos u1d</name>
    <type>TableInput</type>
    <description>exec procedure</description>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>conn13</connection>
    <sql>	DECLARE &#x40;DT_INI AS DATE &#x3d; GETDATE&#x28;&#x29;-1&#xd;&#xa;	DECLARE &#x40;DT_FIM AS DATE &#x3d; GETDATE&#x28;&#x29;-1&#xd;&#xa;&#xd;&#xa;	DECLARE &#x40;TAB_AUTO AS TABLE&#xd;&#xa;	&#x28;&#xd;&#xa;		COD_PEDIDO INT&#xd;&#xa;	&#x29;&#xd;&#xa;	INSERT INTO &#x40;TAB_AUTO&#xd;&#xa;		SELECT DISTINCT&#xd;&#xa;			COD_PEDIDO&#xd;&#xa;		FROM&#xd;&#xa;			BI.DBO.COMPRAS_PEDIDOS AS P&#xd;&#xa;		WHERE FLG_AUTOMATICO &#x3d; 1 AND FLG_TESTE &#x3d; 1 AND CONVERT&#x28;DATE,DATA&#x29; between CONVERT&#x28;DATE,&#x40;DT_INI&#x29; AND CONVERT&#x28;DATE,&#x40;DT_FIM&#x29;&#xd;&#xa;&#xd;&#xa;	SELECT&#xd;&#xa;		P.NUM_PEDIDO as &#x5b;Pedido&#x5d;&#xd;&#xa;		,P.COD_LOJA as &#x5b;Loja&#x5d;&#xd;&#xa;		,SUM&#x28;PP.VAL_TABELA&#x2a;PP.QTD_PEDIDO&#x29; as &#x5b;Vlr Pedido&#x5d;&#xd;&#xa;		,SUM&#x28;PP.VAL_TABELA&#x2a;PP.QTD_RECEBIDA&#x29; AS &#x5b;Vlr Recebido&#x5d;&#xd;&#xa;		--,SUM&#x28;PP.VAL_TABELA&#x2a;PP.QTD_RECEBIDA&#x29;&#x2f;SUM&#x28;PP.VAL_TABELA&#x2a;PP.QTD_PEDIDO&#x29; as &#x5b;Recebida&#x5d;&#xd;&#xa;		,P.COD_PARCEIRO as &#x5b;Cod Forn&#x5d;&#xd;&#xa;		,f.&#x5b;DES_FORNECEDOR&#x5d; as Fornecedor&#xd;&#xa;		--,f.&#x5b;NUM_CGC&#x5d; as CNPJ&#xd;&#xa;		,convert&#x28;date,P.DTA_EMISSAO&#x29; as &#x5b;Data Emissao&#x5d;&#xd;&#xa;		,convert&#x28;date,P.DTA_ENTREGA&#x29; as &#x5b;Data Entrega&#x5d;&#xd;&#xa;		,&#x28;CASE&#xd;&#xa;			WHEN P.&#x5b;TIPO_RECEBIMENTO&#x5d; &#x3d; 1 THEN &#x27;PARCIAL&#x27;&#xd;&#xa;			WHEN P.&#x5b;TIPO_RECEBIMENTO&#x5d; &#x3d; 0 THEN &#x27;PENDENTE&#x27;&#xd;&#xa;			WHEN P.&#x5b;TIPO_RECEBIMENTO&#x5d; &#x3d; 2 THEN &#x27;OK&#x27;&#xd;&#xa;		END&#x29; as &#x5b;Entrega&#x5d;&#xd;&#xa;		,&#x28;CASE WHEN U.NO_COMPRADOR IS NULL THEN &#x27;LOJA&#x27; ELSE U.NO_COMPRADOR END&#x29; as &#x5b;Comprador&#x5d;&#xd;&#xa;		,&#x28;CASE WHEN A.COD_PEDIDO IS NOT NULL THEN 1 ELSE 0 END&#x29; AS FLG_AUTOMATICO&#xd;&#xa;    FROM&#xd;&#xa;		&#x5b;192.168.0.6&#x5d;.&#x5b;ZEUS_RTG&#x5d;.DBO.&#x5b;TAB_PEDIDO&#x5d; AS P&#xd;&#xa;		INNER JOIN &#x5b;192.168.0.6&#x5d;.&#x5b;Zeus_rtg&#x5d;.&#x5b;dbo&#x5d;.&#x5b;TAB_FORNECEDOR&#x5d; AS F&#xd;&#xa;			ON &#x28;f.cod_fornecedor &#x3d; p.cod_parceiro&#x29;&#xd;&#xa;		INNER JOIN &#x5b;192.168.0.6&#x5d;.&#x5b;ZEUS_RTG&#x5d;.DBO.&#x5b;TAB_PEDIDO_PRODUTO&#x5d; AS PP&#xd;&#xa;			ON 1&#x3d;1&#xd;&#xa;			and P.NUM_PEDIDO &#x3d; PP.NUM_PEDIDO&#xd;&#xa;			AND P.COD_LOJA &#x3d; PP.COD_LOJA&#xd;&#xa;			AND P.COD_PARCEIRO &#x3d; PP.COD_PARCEIRO&#xd;&#xa;		LEFT JOIN BI.dbo.COMPRAS_CAD_COMPRADOR AS U&#xd;&#xa;			ON &#x28;P.COD_USUARIO &#x3d; U.COD_USUARIO&#x29;&#xd;&#xa;		LEFT JOIN &#x40;TAB_AUTO AS A&#xd;&#xa;			ON P.NUM_PEDIDO &#x3d; A.COD_PEDIDO&#xd;&#xa;&#xd;&#xa;    WHERE 1 &#x3d; 1&#xd;&#xa;		AND CONVERT&#x28;DATE,DTA_EMISSAO&#x29; between CONVERT&#x28;DATE,&#x40;DT_INI&#x29; AND CONVERT&#x28;DATE,&#x40;DT_FIM&#x29;&#xd;&#xa;		--AND P.COD_LOJA &#x3d; 7&#xd;&#xa;		&#xd;&#xa;    GROUP BY P.NUM_PEDIDO&#xd;&#xa;		,P.COD_LOJA&#xd;&#xa;		,P.COD_PARCEIRO&#xd;&#xa;		,f.&#x5b;DES_FORNECEDOR&#x5d;&#xd;&#xa;		,P.DTA_EMISSAO&#xd;&#xa;		,P.DTA_ENTREGA&#xd;&#xa;		,P.&#x5b;TIPO_RECEBIMENTO&#x5d;&#xd;&#xa;		,&#x28;CASE WHEN U.NO_COMPRADOR IS NULL THEN &#x27;LOJA&#x27; ELSE U.NO_COMPRADOR END&#x29;&#xd;&#xa;		,&#x28;CASE WHEN A.COD_PEDIDO IS NOT NULL THEN 1 ELSE 0 END&#x29;&#xd;&#xa;	order by fornecedor, P.DTA_ENTREGA</sql>
    <limit>0</limit>
    <lookup/>
