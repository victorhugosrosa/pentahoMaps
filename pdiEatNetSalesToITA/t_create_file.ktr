    <sql>SELECT&#xd;&#xa;	&#x27;001&#x27; as PV&#xd;&#xa;	,&#x27;San Paolo&#x27; as PVDESC&#xd;&#xa;	,SUM&#x28;TOTAL_VENDA&#x29;-SUM&#x28;TOTAL_IMPOSTOS&#x29; AS VALUE_BRL&#xd;&#xa;	,CONVERT&#x28;date,DTA_CALCULO&#x29; AS DATA&#xd;&#xa;FROM&#xd;&#xa;	DW.DBO.CMV&#xd;&#xa;WHERE 1&#x3d;1&#xd;&#xa;	and COD_LOJA &#x3d; 29&#xd;&#xa;	AND CONVERT&#x28;DATE,DTA_CALCULO&#x29; between CONVERT&#x28;DATE,GETDATE&#x28;&#x29;-7&#x29; and CONVERT&#x28;DATE,GETDATE&#x28;&#x29;-1&#x29;&#xd;&#xa;GROUP BY&#xd;&#xa;	COD_LOJA&#xd;&#xa;	,CONVERT&#x28;date,DTA_CALCULO&#x29;&#xd;&#xa;	&#xd;&#xa;UNION ALL&#xd;&#xa;	&#xd;&#xa;SELECT&#xd;&#xa;	&#x27;002&#x27; as PV&#xd;&#xa;	,&#x27;San Paolo&#x27; as PVDESC&#xd;&#xa;	,SUM&#x28;ValorTotalSemImposto&#x29; AS VALUE_BRL&#xd;&#xa;	,CONVERT&#x28;date,DT_CUPOM&#x29; AS DATA&#xd;&#xa;FROM&#xd;&#xa;	&#x5b;DW&#x5d;.&#x5b;dbo&#x5d;.&#x5b;TAB_CHEFFSOLUTION_CUPOM&#x5d;&#xd;&#xa;WHERE 1&#x3d;1&#xd;&#xa;	AND CONVERT&#x28;DATE,DT_CUPOM&#x29; between CONVERT&#x28;DATE,GETDATE&#x28;&#x29;-7&#x29; and CONVERT&#x28;DATE,GETDATE&#x28;&#x29;-1&#x29;&#xd;&#xa;	AND COD_LOJA &#x3d; 33&#xd;&#xa;GROUP BY&#xd;&#xa;	CONVERT&#x28;DATE,DT_CUPOM&#x29;&#xd;&#xa;&#xd;&#xa;ORDER BY&#xd;&#xa;1,2,4</sql>
